{
  "name": "textlint-rule-no-synonyms-kurai-forked-ver",
  "version": "2.0.7",
  "description": "同義語を表記ゆれをチェックするtextlintルール",
  "keywords": [
    "textlintrule"
  ],
  "homepage": "https://github.com/TakafumiKurai/textlint-rule-no-synonyms/tree/master/",
  "bugs": {
    "url": "https://github.com/TakafumiKurai/textlint-rule-no-synonyms/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TakafumiKurai/textlint-rule-no-synonyms.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/textlint-rule-no-synonyms.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "textlint-scripts build",
    "prepublish": "npm run --if-present build",
    "test": "textlint-scripts test",
    "watch": "textlint-scripts build --watch",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write"
    ]
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none"
  },
  "dependencies": {
    "@types/tiny-segmenter": "^0.2.0",
    "kuromojin": "^3.0.0",
    "textlint-rule-helper": "^2.1.1",
    "tiny-segmenter": "^0.2.0"
  },
  "devDependencies": {
    "@textlint/types": "^12.0.2",
    "@types/node": "^16.4.13",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "sudachi-synonyms-dictionary-kurai-forked-ver": "^15.0.1",
    "textlint-scripts": "^12.0.2",
    "textlint-tester": "^12.0.2",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "sudachi-synonyms-dictionary-kurai-forked-ver": "^15.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
